#квадратное уравнение
print('введите параметры a,b,c в уравнении ax**2+bx+c=0')
print('a:')
a=int(input())
print('b:')
b=int(input())
print('c:')
c=int(input())
x1 = (-b+(b ** 2-4*c*a)**0.5)/2/a
x2 = (-b-(b ** 2-4*c*a)**0.5)/2/a
if b ** 2-4*c*a == 0:
    print ('Ответ: ','x= ',x1)
else:
    print ('Ответ: ','x1= ',x1,'x2= ',x2)
